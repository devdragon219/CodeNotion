<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="XPlat code coverage">
        <Configuration>
          <Format>cobertura</Format>
          <ExcludeByFile>**/Migrations/*.cs</ExcludeByFile>
          <Exclude>[*]RealGimm.FunctionalTests*</Exclude>
          <Exclude>[*]RealGimm.UnitTests*</Exclude>
          <Exclude>[*]RealGimm.IntegrationTests*</Exclude>
          <Exclude>[*]RealGimm.ImportTests*</Exclude>
          <ExcludeByAttribute>TestSDKAutoGeneratedCode</ExcludeByAttribute>
        </Configuration>
      </DataCollector>
    </DataCollectors>
  </DataCollectionRunSettings>
</RunSettings>